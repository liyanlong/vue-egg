  "private": true,
  "scripts": {
    "client:dev": "node client/build/dev-server.js",
    "client:build": "node client/build/build.js",
    "client:unit": "cross-env BABEL_ENV=test karma start test/client/unit/karma.conf.js --single-run",
    "client:e2e": "node test/client//e2e/runner.js",
    "client:test": "npm run client:unit && npm run client:e2e",
    "client:lint": "eslint --ext .js,.vue client/src test/client/unit/specs test/client/e2e/specs",
    "server:dev": "egg-bin dev --baseDir=server --port=3000",
    "dev": "npm-run-all --parallel server:dev client:dev",
    "start": "egg-scripts start ./server --port=7001 --daemon",
    "stop": "egg-scripts stop ./server",
    "debug": "egg-bin debug --baseDir=server --port=5858"
  },
